Date date = new Date();
        date.setHours(1);
        date.setMinutes(30);
        date.setSeconds(0);

        //Station
        Station station1 = new Station(1, "Estación A", StationType.R, 10);
        Station station2 = new Station(2, "Estación B", StationType.T, 15);
        Station station3 = new Station(3, "Estación C", StationType.M, 12);
        Station station4 = new Station(4, "Estación D", StationType.C, 8);
        Station station5 = new Station(5, "Estación E", StationType.R, 10);
        Station station6 = new Station(6, "Estación F", StationType.T, 15);

        //Section
        Section section1 = new Section(station1, station2, 20.5f, 30.0f);
        Section section2 = new Section(station2, station3, 15.2f, 25.0f);
        Section section3 = new Section(station3, station4, 18.0f, 28.5f);
        Section section4 = new Section(station4, station5, 20.0f, 30.0f);
        Section section5 = new Section(station5, station1, 20.0f, 30.0f);

        //Section List
        List<Section> seccionesLinea1 = new ArrayList<>();
        seccionesLinea1.add(section1);
        seccionesLinea1.add(section2);
        seccionesLinea1.add(section3);
        seccionesLinea1.add(section4);
        seccionesLinea1.add(section5);

        List<Section> seccionesLinea2 = new ArrayList<>();
        seccionesLinea2.add(section1);
        seccionesLinea2.add(section2);
        seccionesLinea2.add(section3);
        seccionesLinea2.add(section4);

        //Line
        Line linea1 = new Line(1, "Línea 1", "F-200", seccionesLinea1);
        Line linea2 = new Line(2, "Linea 2", "F-300", seccionesLinea2);

        //Lista de lineas
        List<Line> lineList = new ArrayList<>();
        lineList.add(linea1);

        List<Line> lineList1 = new ArrayList<>();
        lineList1.add(linea1);
        lineList1.add(linea2);

        //PassengerCar

        PassengerCar passengerCar1 = new PassengerCar(1, 300, "UWU", "SEBA", CarType.TR);
        PassengerCar passengerCar2 = new PassengerCar(2, 200, "UWU", "SEBA", CarType.CT);
        PassengerCar passengerCar3 = new PassengerCar(3, 400, "UWU", "SEBA", CarType.CT);
        PassengerCar passengerCar4 = new PassengerCar(4, 500, "UWU", "SEBA", CarType.TR);
        PassengerCar passengerCar5 = new PassengerCar(5, 600, "UWU", "SEBA", CarType.TR);

        //PassergerCarlist

        List<PassengerCar> passengerCarList = new ArrayList<>();
        passengerCarList.add(passengerCar1);
        passengerCarList.add(passengerCar2);
        passengerCarList.add(passengerCar3);
        passengerCarList.add(passengerCar4);

        List<PassengerCar> passengerCarList1 = new ArrayList<>();
        passengerCarList1.add(passengerCar1);
        passengerCarList1.add(passengerCar2);
        passengerCarList1.add(passengerCar3);
        passengerCarList1.add(passengerCar5);

        //Train

        Train train1 = new Train(1,"UWU2",300, passengerCarList);
        Train train2 = new Train(2,"EsteSech",500, passengerCarList1);

        //ListTrain
        List<Train> trainList = new ArrayList<>();
        trainList.add(train1);

        List<Train> trainList1 = new ArrayList<>();
        trainList1.add(train1);
        trainList1.add(train2);

        //Driver

        Driver driver = new Driver(1, "Johan", "UWU");
        Driver driver1 = new Driver(2, "Nelson", "UWU1");
        Driver driver2 = new Driver(2, "Pipe", "UWU2");

        //DriverList
        List <Driver> driverList = new ArrayList<>();
        driverList.add(driver);
        driverList.add(driver1);

        List<Driver> driverList1 = new ArrayList<>();
        driverList1.add(driver2);

