        //Crear Subway
        Subway subway1 = new Subway(1,"Elpepe");
        subway1.addTrain(trainList);
        subway1.addTrain(trainList1);
        subway1.addDriver(driverList);
        subway1.addDriver(driverList1);
        subway1.addLine(lineList);
        subway1.addLine(lineList1);
        subway1.assignTrainToLine(train1, linea1);
        subway1.assignDriverToTrain(train1, driver2, date, station1, station6);